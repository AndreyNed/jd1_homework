package by.htp.blinov_p126_5b;

public class Record extends Song{

	private String discTitle;
	private int Index;
	
	public Record(Song song, String discTitle, int index) {
		super(song.getTitle(), song.getArtist(), song.getGenre());
		this.discTitle = discTitle;
		Index = index;
	}

	public String getDiscTitle() {
		return discTitle;
	}

	public void setDiscTitle(String discTitle) {
		this.discTitle = discTitle;
	}

	public int getIndex() {
		return Index;
	}

	public void setIndex(int index) {
		Index = index;
	}
	
	

	
	
	
}
